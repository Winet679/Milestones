<?xml version="1.0"?>
<robot name="Winet">

  <material name="blue">
    <color rgba="0.047 0.722 0.961 1"/>
  </material>
  <material name="yellow">
    <color rgba="1 1 0 1"/>
  </material>
  <material name="green">
    <color rgba="0.067 0.922 0.208 1"/>
  </material>
  <material name="pink">
    <color rgba="0.902 0.11 0.7921 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.9" radius="0.04"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.45"/>
      <material name="blue"/>
    </visual>
  </link>

  <link name="arm1">
    <visual>
      <geometry>
        <cylinder length="0.7" radius="0.04"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.35"/>
      <material name="yellow"/>
    </visual>
  </link>

  <joint name="base_link_to_arm1" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.57079632679" upper="1.57079632679" velocity="0.5"/>
    <origin rpy="0 0 0" xyz="0 0 0.9"/>
    <parent link="base_link"/>
    <child link="arm1"/>
  </joint>

  <link name="arm2">
    <visual>
      <geometry>
        <cylinder length="0.5" radius="0.04"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.25"/>
      <material name="green"/>
    </visual>
  </link>

  <joint name="arm1_arm2" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.57079632679" upper="1.57079632679" velocity="0.5"/>
    <origin rpy="0 0 0" xyz="0 0 0.7"/>
    <parent link="arm1"/>
    <child link="arm2"/>
  </joint>

  <link name="arm3">
    <visual>
      <geometry>
        <cylinder length="0.3" radius="0.04"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.15"/>
      <material name="pink"/>
    </visual>
  </link>

  <joint name="arm2_arm3" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.57079632679" upper="1.57079632679" velocity="0.5"/>
    <origin rpy="0 0 0" xyz="0 0 0.5"/>
    <parent link="arm2"/>
    <child link="arm3"/>
  </joint>

  <link name="end_effector"/>

  <joint name="arm3_end_effector" type="fixed">
    <parent link="arm3"/>
    <child link="end_effector"/>
    <origin rpy="0 0 0" xyz="0 0 0.3"/>
  </joint>

</robot>